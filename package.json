{
  "type": "module",
  "name": "yneo-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "prisma generate && next build",
    "dev:docker": "cp .env.local .env && npx prisma migrate dev && npm run dev",
    "dev:neon": "cp .env.production .env && npx prisma migrate deploy && npm run dev",
    "start": "next start",
    "lint": "next lint",
    "studio:local": "node studio.js local",
    "studio:neon": "node studio.js neon",
    "export": "next build && next export",
    "deploy": "gh-pages -d out",
    "predeploy": "npm run export"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
