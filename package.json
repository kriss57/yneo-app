    "@prisma/client": "^6.14.0",
    "@vercel/blob": "^1.1.1",
    "aos": "^2.3.4",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.22",
    "media-chrome": "^4.13.1",
    "next": "15.4.6",
    "next-auth": "^5.0.0-beta.29",
    "react": "19.1.0",
